{
  "version": "0.4.18",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "api": "rm -rf src/api && mkdir src/api && openapi-generator-cli generate -g typescript-axios -i https://api.gateway1.dev.trustcerts.de/api-json -o src/api --api-name-suffix=GatewayApi --additional-properties=supportsES6=true,enumPropertyNaming=original,enumNameSuffix="
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@trustcerts/gateway",
  "author": "trustcerts",
  "module": "dist/gateway.esm.js",
  "size-limit": [
    {
      "path": "dist/gateway.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gateway.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.23",
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}

    {
      "path": "dist/schema-verify.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@trustcerts/core": "^0.4.17",
    "@trustcerts/gateway": "^0.4.17",
    "@trustcerts/observer": "^0.4.17"

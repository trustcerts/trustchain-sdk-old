{
	"version": "0.0.6",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=15"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"name": "@trustcerts/core",
	"author": "Mirko Mollik",
	"module": "dist/common.esm.js",
	"size-limit": [
		{
			"path": "dist/common.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/common.esm.js",
			"limit": "10 KB"
		}
	],
	"jest": {
		"transform": {
			".(ts|tsx)": "ts-jest"
		},
		"testEnvironment": "node",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"moduleNameMapper": {
			"^jose/(.*)$": "<rootDir>/node_modules/jose/dist/node/cjs/$1"
		},
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/test/"
		],
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": 0
			}
		},
		"collectCoverageFrom": [
			"src/*.{js,ts}"
		]
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.4.7",
		"@size-limit/preset-small-lib": "^5.0.5",
		"@types/axios": "^0.14.0",
		"@types/bs58": "^4.0.1",
		"husky": "^7.0.2",
		"size-limit": "^5.0.5",
		"tsdx": "^0.14.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"@mattrglobal/bls12381-key-pair": "^0.7.0",
		"@mattrglobal/jsonld-signatures-bbs": "^0.11.0",
		"axios": "^0.24.0",
		"bs58": "^4.0.1",
		"jsonld-signatures": "^9.3.0"
	},
	"gitHead": "9cc3c3d078fe46729a72961bc6bd241667d43309"
}
